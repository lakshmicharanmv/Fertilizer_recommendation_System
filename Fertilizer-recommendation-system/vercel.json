{
  "builds": [
    {
      "src": "FertilizerApp/Fertilizer/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/FertilizerApp/Fertilizer/wsgi.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
